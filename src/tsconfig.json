{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"strict": true,
		"declaration": true,
		"removeComments": false,
		"sourceMap": true,
		"noLib": false,
		"preserveConstEnums": true,
		"suppressImplicitAnyIndexErrors": true,
		"moduleResolution": "node",
		"outDir": "../dist/",
		"rootDir": ".",
		"types": ["node"],
		"lib": ["es6", "es2015.promise"]
	}
}
